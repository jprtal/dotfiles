{
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "telemetry.telemetryLevel": "off",
    "workbench.enableExperiments": false,
    "window.restoreWindows": "none",
    "workbench.startupEditor": "newUntitledFile",
    "files.hotExit": "off",
    "window.titleBarStyle": "custom",
    "workbench.iconTheme": "material-icon-theme",
    "[dart]": {
        "editor.formatOnSave": false,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },
    "dart.flutterCreateAndroidLanguage": "java",
    "dart.allowAnalytics": false,
    "platformio-ide.activateOnlyOnPlatformIOProject": true,
    "platformio-ide.disablePIOHomeStartup": true,
    "terminal.integrated.fontFamily": "'MesloLGS Nerd Font'",
    "workbench.editor.untitled.hint": "hidden",
    "C_Cpp.autocompleteAddParentheses": true,
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4 }",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/**",
        "/usr/include/**",
        "/usr/lib64/**"
    ],
    "editor.fontFamily": "'JetBrains Mono', 'Fira Code', 'Noto Sans Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": true,
    "editor.fontSize": 13.5,
}
